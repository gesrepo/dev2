    public void doInvoke(
        Object stub,
        Object request,
        Map<String, Object> response,
        String endpoint,
        String soapAction,
        String requestName,
        String responseNS,
        String responseName,
        String responseType) {
            
            System.debug(LoggingLevel.INFO, 'SQRServiceClientMockImpl.doInvoke() - ' +
                         '\n request: ' + request +
                         '\n response: ' + response +
                         '\n endpoint: ' + endpoint +
                         '\n soapAction: ' + soapAction +
                         '\n requestName: ' + requestName +
                         '\n responseNS: ' + responseNS +
                         '\n responseName: ' + responseName +
                         '\n responseType: ' + responseType);
            
            // Create Session
            if (requestName == 'StartAppSession'){
                wwwViadComSecurityV2.StartAppSessionResponse_element session = new wwwViadComSecurityV2.StartAppSessionResponse_element();
                wwwViadComSecurityV2.AppSessionInfo sessioninfo = new wwwViadComSecurityV2.AppSessionInfo();
                // Populate response element.
                sessioninfo.SessionID = 'jhdhkfhkdf';
                session.StartAppSessionResult = sessioninfo;
                // Add response element to the response parameter, as follows:
                response.put('response_x', session);
            }
            if (requestName == 'UserLogin'){
                wwwViadComSecurityV2.UserLoginResponse_element loginresponse= new wwwViadComSecurityV2.UserLoginResponse_element();
                wwwViadComSecurityV2.AppSessionInfo logininfo = new wwwViadComSecurityV2.AppSessionInfo();
                logininfo.SessionID = 'jhdhkfhkdf';
                //logininfo.loginName = 'amarab2486';
                //logininfo.Password = 'welcome#123'; 
                
                loginresponse.UserLoginResult = logininfo;
                response.put('response_x', loginresponse);
            }
            // Create response element from the autogenerated class.
            if (requestName == 'AddShowSiteContact'){
                wwwViadComV1_ExhibitorService.AddShowSiteContactResponse_element responseElement=new wwwViadComV1_ExhibitorService.AddShowSiteContactResponse_element();
                // Populate response element.
                responseElement.AddShowSiteContactResult = 123456;                  
                // Add response element to the response parameter, as follows:
                response.put('response_x', responseElement);
            }
            if (requestName == 'UpdateShowSiteContact'){
                wwwViadComV1_ExhibitorService.UpdateShowSiteContactResponse_element updresponseElement=new wwwViadComV1_ExhibitorService.UpdateShowSiteContactResponse_element();
                // Populate response element.
                updresponseElement.UpdateShowSiteContactResult = true;                  
                // Add response element to the response parameter, as follows:
                response.put('response_x', updresponseElement);
            }
        }
}