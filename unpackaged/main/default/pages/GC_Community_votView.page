                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-no-flex">
                                    <div class="slds-grid">
                                        <div class="slds-button">
                                            <apex:commandButton title=" Click to go back to the order" action="{!URLFOR($Page.GC_Community_OrderView,null,[orderId=orderId,showId=showId])}" value="Back to Order" id="BackToOrderButton2" styleClass="slds-button slds-button--brand gc-teal-button"/>
                                            <apex:commandButton action="{!editRecord}" value="Edit" id="editRecord" styleClass="slds-button slds-button--brand gc-teal-button" rendered="{!CanEdit}" />
