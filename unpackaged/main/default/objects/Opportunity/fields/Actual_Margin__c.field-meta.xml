<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Actual_Margin__c</fullName>
    <description>D&amp;B actual margin</description>
    <formula>IF(OR( Amount = 0, Margin__c =0),0, Amount * Margin__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>amount / margin * 100</inlineHelpText>
    <label>Actual Margin</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
