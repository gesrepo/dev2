<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Last_Stage_Date__c</fullName>
    <description>Identifies date of new stages or when stage was changed.</description>
    <formula>IF(   ISBLANK(DATETIMEVALUE(LastStageChangeDate)) = TRUE ,   DATETIMEVALUE(CreatedDate) , DATETIMEVALUE(LastStageChangeDate))</formula>
    <label>Last Stage Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
