<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Do_not_allow_0_on_Discount</fullName>
    <active>true</active>
    <description>Users cannot put a 0 in Discount</description>
    <errorConditionFormula>Booth_Area_Discount_Override__c = 0</errorConditionFormula>
    <errorDisplayField>Booth_Area_Discount_Override__c</errorDisplayField>
    <errorMessage>Booth Area Discount cannot be zero.</errorMessage>
</ValidationRule>
