<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Exhibitor_Marketing_Preferences__c</fullName>
    <description>This field identifies what email preferences the show organizer has selected for us to email exhibitors before a show.</description>
    <inlineHelpText>Standard Email Package: US = 3 to 5 Emails
Extended Email Package: US = 5 to 7 Emails
Decline Email Marketing - The organizer does not want GES to email exhibitors</inlineHelpText>
    <label>Exhibitor Marketing Preferences</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Standard Email Package</fullName>
                <default>true</default>
                <label>Standard Email Package</label>
            </value>
            <value>
                <fullName>Extended Email Package</fullName>
                <default>false</default>
                <label>Extended Email Package</label>
            </value>
            <value>
                <fullName>Decline Email Marketing</fullName>
                <default>false</default>
                <label>Decline Email Marketing</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
