<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ICS_SalesOrderNumber</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(isblank(Sales_Order__c)),NOT(ISNUMBER(  Sales_Order__c  )))</errorConditionFormula>
    <errorDisplayField>Sales_Order__c</errorDisplayField>
    <errorMessage>Please Verify the Sales Order #</errorMessage>
</ValidationRule>
