<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Include_internal_and_external_links</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(  Is_it_for_both_inbound_and_or_outbound__c , &#39;Yes&#39;) , ISBLANK( Include_internal_and_external_links__c ) )</errorConditionFormula>
    <errorDisplayField>Include_internal_and_external_links__c</errorDisplayField>
    <errorMessage>Include internal and external links to the maps.</errorMessage>
</ValidationRule>
