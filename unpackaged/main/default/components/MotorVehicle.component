            <!-- Additional Comments -->
            <apex:PageBlockSection title="{!$Label.Additional_Comments}" columns="1">
                <apex:inputField style="width:98%;" value="{!IncidentReport.Additional_Comments__c}" />
            </apex:PageBlockSection>

            <!-- Save Button -->
            <apex:pageBlockButtons location="bottom">
                <apex:commandButton action="{!Controller.Save}" value="{!ButtonLabel}" />
                <apex:commandButton action="{!Controller.saveWithoutSubmission}" value="{!$Label.SaveWithoutSubmit}" />
